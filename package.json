  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/**/*.{json,jsx,ts,tsx}": [
      "eslint --config .eslintrc.js --fix"
    ],
    "*.{ts,tsx,json,js,jsx,html,yml,md,mdx}": [
      "prettier --config .prettierrc.js --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "directories": {
    "lib": "lib"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "start": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.config.js --env env=dev",
    "build-android": "webpack --config webpack.config.js --env env=dev && npx cap add android && npx cap open android",
    "build-ios": "webpack --config webpack.config.js --env env=dev && npx cap add ios && npx cap open ios",
    "sync-android": "npx cap sync android",
    "sync-ios": "npx cap sync ios",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "convex:dashboard": "convex dashboard",
    "convex:logs": "convex logs",
    "dev": "concurrently \"npm run start\" \"npm run convex:dev\"",
    "deploy": "npm run convex:deploy && npm run build",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{ts,tsx,js,jsx,html,yml,md}\"",
    "format:check": "prettier --config ./.prettierrc.json --check \"**/*.{ts,tsx,js,jsx,html,yml,md}\"",
    "lighthouse": "npm run build && lhci autorun --collect.startServerCommand=\"npm run lighthouse:serve\" --collect.startServerReadyPattern=\"Server ready at port 3000\" && node open-lighthouse-report.js",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "lighthouse:serve": "node server.js",
    "naming:convention": "node .github/scripts/naming-convention.js",
    "update:deps": "node .github/scripts/update-cicd-deps.js",
    "precommit:check": "node .github/scripts/universal-check.js",
    "trivy:scan": "node .github/scripts/trivy-scan.js"
  }
